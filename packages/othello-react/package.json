{
  "name": "othello",
  "version": "0.2.0",
  "homepage": "https://cozygarage.github.io/Othello/",
  "private": true,
  "type": "module",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "othello-engine": "workspace:*"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "predeploy": "bun run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.2.0",
    "gh-pages": "^5.0.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^27.0.1",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
